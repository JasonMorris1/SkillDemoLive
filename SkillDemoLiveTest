
import org.junit.*;
import static org.junit.Assert.*;

public SkillDemoLiveTest
{

    @Test
    public void testTwoSum()
    {
       int[] nums =  {2,7,11,15};

       int[] expected = {0 , 1};
       int target = 12;

       int[] output = SkillDemoLive.twoSum(nums, target);

       assertArrayEquals(expected, output);

}